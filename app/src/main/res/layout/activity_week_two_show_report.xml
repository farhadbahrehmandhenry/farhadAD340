<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WeekTwoShowReportActivity">

    <TextView
        android:id="@+id/weekTwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/my_margin"
        android:layout_marginTop="@dimen/my_margin"
        android:layout_marginEnd="@dimen/my_margin"
        android:text="@string/week_two_form"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/projrctName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/my_margin"
        android:text="@string/bahrehmand_farhad_04_22_2020"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/weekTwo" />

    <TextView
        android:id="@+id/report"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/my_margin"
        android:layout_marginEnd="@dimen/my_margin"
        android:layout_marginBottom="@dimen/my_margin"
        android:textAlignment="center"
        android:textColor="@android:color/background_dark"
        android:textSize="@dimen/my_textSize"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/backButton"
        />

    <Button
        android:id="@+id/backButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/my_margin"
        android:layout_marginTop="@dimen/my_margin"
        android:layout_marginEnd="@dimen/my_margin"
        android:onClick="backToForm"
        android:text="@string/back_to_form"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/projrctName" />

</androidx.constraintlayout.widget.ConstraintLayout>